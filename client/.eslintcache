(token, user_id) {\n    dispatch({\n      type: STORE_TOKEN_AND_USER_ID,\n      payload: { token, user_id },\n    });\n  }\n\n  function createReviewPost(reviewData) {\n    dispatch({\n      type: CREATE_REVIEW_POST,\n      payload: reviewData,\n    });\n  }\n\n  function storeAllMovieReviews(reviewList) {\n    dispatch({\n      type: GET_ALL_MOVIE_REVIEWS,\n      payload: reviewList,\n    });\n  }\n\n  return (\n    <GlobalContext.Provider\n      value={{\n        user_token: state.user_token,\n        user_id: state.user_id,\n        isAuthenticated: state.isAuthenticated,\n        clearAuth,\n        storeAuth,\n      }}\n    >\n      {children}\n    </GlobalContext.Provider>\n  );\n};\n","/Users/anantawasthy/Documents/reactWorkspace/movie-review-app/client/src/global/AppReducer.js",[],"/Users/anantawasthy/Documents/reactWorkspace/movie-review-app/client/src/global/types.js",[],"/Users/anantawasthy/Documents/reactWorkspace/movie-review-app/client/src/PrivateRouteHome.js",[],{"ruleId":"146","replacedBy":"147"},{"ruleId":"148","replacedBy":"149"},{"ruleId":"150","severity":1,"message":"151","line":32,"column":6,"nodeType":"152","endLine":32,"endColumn":8,"suggestions":"153"},{"ruleId":"154","severity":1,"message":"155","line":1,"column":17,"nodeType":"156","messageId":"157","endLine":1,"endColumn":25},{"ruleId":"154","severity":1,"message":"158","line":2,"column":10,"nodeType":"156","messageId":"157","endLine":2,"endColumn":14},{"ruleId":"150","severity":1,"message":"159","line":28,"column":6,"nodeType":"152","endLine":28,"endColumn":8,"suggestions":"160"},{"ruleId":"146","replacedBy":"161"},{"ruleId":"148","replacedBy":"162"},{"ruleId":"154","severity":1,"message":"163","line":10,"column":10,"nodeType":"156","messageId":"157","endLine":10,"endColumn":15},{"ruleId":"164","severity":1,"message":"165","line":16,"column":40,"nodeType":"166","messageId":"167","endLine":16,"endColumn":42},{"ruleId":"154","severity":1,"message":"168","line":40,"column":12,"nodeType":"156